name: 彩云unlock
desc: 彩云unlock

http:
  mitm:
    - "%APPEND% biz.caiyunapp.com"

  script:
    - match: ^https:\/\/biz\.caiyunapp\.com\/v2\/user\?app_name\=weather
      name: weather
      type: response
      require-body: true
      timeout: 10

script-providers:
  weather:
    url: https://raw.githubusercontent.com/Maasea/surgeModule/master/Script/Weather/weather.js
    interval:  86400
