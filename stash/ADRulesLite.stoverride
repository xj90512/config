    type: http
    behavior: domain
    url: "https://ghproxy.com/https://raw.githubusercontent.com/Loyalsoldier/clash-rules/release/reject.txt"
    path: ./ruleset/AdDomain.yaml
    interval: 86400
    
rules:
  - RULE-SET,Adblock-Domain,REJECT